<section class="page__section project__menu">
    <div class="menu">
        <div class="menu__item menu__item_type_burger dropdown dropdown_on_hover">
            <div class="dropdown__trigger nostyle">
                <i class="icon icon__burger icon_hover_on icon_size_l"></i>
            </div>
            <div class="dropdown__context">
                <span class="dropdown__item active">Проекты</span>
                <a href="#/docs/" class="dropdown__item link blue">Документация</a>
                <a href="#/settings/" class="dropdown__item link blue">Настройки</a>
            </div>
        </div>
        <a href="/#" class="menu__item active menu__header link style_black js_menu_item">Проекты</a>
        {{#each links}}
            <a href="/#" data-url="{{url}}" class="menu__item link js_menu_item">{{name}}</a>
        {{/each}}
        <div class="menu__item style_float_right dropdown dropdown_on_hover">
            <div class="dropdown__trigger nostyle noalign">
                Создать
                <i class="icon icon__chevron icon_rotate_180 icon_size_xs"></i>
            </div>
            <div class="dropdown__context dropdown__context_position_right">
                <span class="dropdown__item js_create_project link blue">Проект</span>
            </div>
        </div>
        <a href="#/profile/" class="link menu__item style_float_right js_menu_item">Антон Ахатов</a>
    </div>
</section>