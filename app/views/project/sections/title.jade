mixin project-title(header)
	.title
		.title__header
			each item,i in header.title
				if i == header.title.length-1
					span.style_size_l.style_bold= item.name
				else 
					a(href=item.url).link.style_solid.style_gray= item.name
					span.style_delimiter.style_gray /
			.title__label
				+ui-label(header.label.color, header.label.name)